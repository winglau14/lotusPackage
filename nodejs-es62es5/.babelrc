{
  "presets": [
    "es2015",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        // defaults to true
        "polyfill": false,
        // defaults to true
        "regenerator": true,
        // defaults to true
        "moduleName": "babel-runtime"
        // defaults to "babel-runtime"
      }
    ]
  ]
}
